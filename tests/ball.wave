onerror {resume}
quietly WaveActivateNextPane {} 0
add wave -noupdate /ball_testbench/CLOCK_PERIOD
add wave -noupdate /ball_testbench/clk
add wave -noupdate /ball_testbench/reset
add wave -noupdate -radix unsigned /ball_testbench/x
add wave -noupdate -radix unsigned /ball_testbench/y
add wave -noupdate /ball_testbench/speed_x
add wave -noupdate /ball_testbench/speed_y
add wave -noupdate -color Coral /ball_testbench/ball_test/ball_x/add
add wave -noupdate -color Coral /ball_testbench/ball_test/ball_x/subtract
add wave -noupdate -color Coral -radix unsigned /ball_testbench/ball_test/ball_x/set
add wave -noupdate -color Coral -radix unsigned /ball_testbench/ball_test/ball_x/limit
add wave -noupdate -color Coral -radix unsigned /ball_testbench/ball_test/ball_x/out
add wave -noupdate -color Cyan /ball_testbench/ball_test/ball_y/add
add wave -noupdate -color Cyan /ball_testbench/ball_test/ball_y/subtract
add wave -noupdate -color Cyan -radix unsigned /ball_testbench/ball_test/ball_y/set
add wave -noupdate -color Cyan -radix unsigned /ball_testbench/ball_test/ball_y/limit
add wave -noupdate -color Cyan -radix unsigned /ball_testbench/ball_test/ball_y/out
TreeUpdate [SetDefaultTree]
WaveRestoreCursors {{Cursor 1} {296 ps} 0}
quietly wave cursor active 1
configure wave -namecolwidth 150
configure wave -valuecolwidth 100
configure wave -justifyvalue left
configure wave -signalnamewidth 0
configure wave -snapdistance 10
configure wave -datasetprefix 0
configure wave -rowmargin 4
configure wave -childrowmargin 2
configure wave -gridoffset 0
configure wave -gridperiod 1
configure wave -griddelta 40
configure wave -timeline 0
configure wave -timelineunits ps
update
WaveRestoreZoom {0 ps} {1 ns}
